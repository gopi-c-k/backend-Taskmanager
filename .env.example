# Server Port
PORT=5000

# MongoDB Connection URI (replace <dbpassword> with your actual password)
MONGO_URI=mongodb+srv://<username>:<dbpassword>@<cluster>.mongodb.net/?retryWrites=true&w=majority&appName=<appName>

# JWT Secrets (use strong random strings in production)
ACCESS_TOKEN_SECRET=<your_access_token_secret>
REFRESH_TOKEN_SECRET=<your_refresh_token_secret>

# Node Environment
NODE_ENV=development
